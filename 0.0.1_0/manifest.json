{
   "background": {
      "persistent": true,
      "scripts": [ "request.js" ]
   },
   "browser_action": {
      "default_icon": "packet proxy.png",
      "default_popup": "index.html",
      "default_title": "Choose your proxy."
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+Shift+U",
            "linux": "Ctrl+Shift+J",
            "mac": "Command+Shift+Y",
            "windows": "Ctrl+Shift+Y"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Setup custom redirect rules for any http request i.e pages, scripts, images etc. Uses a simple string replace to do this.",
   "key": "",
   "manifest_version": 2,
   "name": "Packet Proxy",
   "permissions": [ "webRequest", "webRequestBlocking", "\u003Call_urls>", "tabs", "notifications", "cookies" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
